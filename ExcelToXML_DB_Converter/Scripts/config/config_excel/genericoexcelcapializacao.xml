<config>
    <database>
        <server>localhost</server>
        <port>1433</port>
        <database_name>master</database_name>
        <trusted_connection>yes</trusted_connection>
        <table name="Tabela_Capitalizacao_RV">
            <columns>
                <column name="Empresa" type="NVARCHAR(255)" source_name="Empresa" default="N/A"/>
                <column name="Mes_Processamento" type="NVARCHAR(50)" source_name="Mês Processamento" default=""/>
                <column name="Tipo_Novo_ou_Reforco" type="NVARCHAR(50)" source_name="Tipo - novo ou reforço(N/R)" default=""/>
                <column name="Numero_Fiscal" type="NVARCHAR(50)" source_name="Numero Fiscal" default=""/>
                <column name="Nome_do_aderente" type="NVARCHAR(255)" source_name="Nome do aderente" default=""/>
                <column name="Num_ID" type="NVARCHAR(50)" source_name="nºid" default=""/>
                <column name="Email" type="NVARCHAR(255)" source_name="email" default=""/>
                <column name="Premio_Unique_Reforco" type="DECIMAL(18,2)" source_name="Prémio (Único/Reforço)" default="0.00"/>
                <column name="Custo_Apolice" type="DECIMAL(18,2)" source_name="Custo Apólice" default="0.00"/>
                <column name="Comissao_de_subscricao" type="DECIMAL(18,2)" source_name="Comissão de subscrição" default="0.00"/>
                <column name="Premio_Total" type="DECIMAL(18,2)" source_name="Prémio Total" default="0.00"/>
                 <column name="Data_Hora" type="DATETIME" source_name="Data_Hora" default="" />
            </columns>
        </table>
    </database>
    <excel>
        <file_path>ASK</file_path>
        <sheet_name>SV_Capitalização</sheet_name>
    </excel>
</config>

